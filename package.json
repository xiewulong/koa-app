{
  "name": "koa-app",
  "version": "0.0.1",
  "description": "Koa app template",
  "private": true,
  "main": "app.js",
  "scripts": {
    "app": "node ./app",
    "build": "npm run webpack -- --display-error-details --mode=production --optimize-minimize",
    "dev": "npm run webpack -- --debug --devtool sourcemap --mode=development --output-pathinfo --watch",
    "release": "npm run build",
    "restart": "touch ./tmp/restart.txt",
    "service": "docker-compose up -d",
    "service:down": "docker-compose down",
    "service:down:all": "docker-compose down --rmi -v all",
    "service:rebuild": "docker-compose up --build -d && docker system prune --volumes -f",
    "service:recreate": "docker-compose up --force-recreate -d",
    "service:restart": "docker-compose restart",
    "service:stop": "docker-compose stop",
    "test": "mocha",
    "start": "npm run dev",
    "webpack": "webpack --colors --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiewulong/koa-app.git"
  },
  "keywords": [
    "koa",
    "app",
    "template"
  ],
  "author": "xiewulong <xiewulong@vip.qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiewulong/koa-app/issues"
  },
  "homepage": "https://github.com/xiewulong/koa-app#readme",
  "dependencies": {
    "activerequest": "^0.0.2",
    "axios": "^0.18.0",
    "js-yaml": "^3.10.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-captcha-v2": "^0.0.7",
    "koa-csrf": "^3.0.6",
    "koa-devise": "^0.0.9",
    "koa-flash": "^1.0.0",
    "koa-i18n": "^2.1.0",
    "koa-locale": "^1.3.0",
    "koa-mailer-v2": "^0.0.3",
    "koa-mongo": "^1.3.0",
    "koa-mongo-bucket": "^0.0.4",
    "koa-multer": "^1.0.2",
    "koa-pug": "^3.0.0-2",
    "koa-rbac": "^2.0.1",
    "koa-redis": "^3.1.1",
    "koa-router": "^7.3.0",
    "koa-session": "^5.5.1",
    "koa-static": "^4.0.2",
    "log4js": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-sprites": "^4.2.1",
    "qs": "^6.5.2",
    "sass-loader": "^7.0.3",
    "swagger-ui": "^3.17.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  }
}
